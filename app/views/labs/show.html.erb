
<% if current_user.admin? %>
	<%= link_to 'Edit', edit_lab_path(@lab) %> |
	<%= link_to 'Back', labs_path %>
	<br>
<% end %>


<p>
  <strong>Name:</strong>
  <%= @lab.name %>
</p>

<p>
  <strong>College:</strong>
  <%= @lab.college %>
</p>

<% @lab.documents.each do |document| %>
	<%= link_to document.name, document.doc_file.url %>
<% end %>
